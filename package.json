{
  "name": "openapi-to-mcp-server",
  "version": "1.0.0",
  "description": "将 OpenAPI 规范转换为 MCP Server 配置的 Web 服务",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "release": "bash scripts/release.sh",
    "release:patch": "bash scripts/release.sh patch",
    "release:minor": "bash scripts/release.sh minor",
    "release:major": "bash scripts/release.sh major",
    "docker:build": "bash scripts/docker-build.sh",
    "docker:push": "bash scripts/docker-push.sh",
    "version:bump": "bash scripts/bump-version.sh"
  },
  "keywords": [
    "openapi",
    "mcp",
    "api",
    "converter",
    "model-context-protocol",
    "ai-tools",
    "api-gateway"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NNNNzs/openapi-to-mcp-server"
  },
  "bugs": {
    "url": "https://github.com/NNNNzs/openapi-to-mcp-server/issues"
  },
  "homepage": "https://github.com/NNNNzs/openapi-to-mcp-server#readme",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

